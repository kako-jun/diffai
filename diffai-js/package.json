{
  "name": "diffai-js",
  "version": "0.3.16",
  "description": "A Node.js wrapper for the diffai CLI tool - AI/ML specialized diff tool for PyTorch, Safetensors, NumPy, and MATLAB files with deep tensor analysis",
  "keywords": [
    "ai",
    "ml",
    "machine-learning",
    "diff",
    "tensor",
    "pytorch",
    "safetensors",
    "numpy",
    "matlab",
    "model-comparison",
    "diffai",
    "artificial-intelligence",
    "cli",
    "deep-learning",
    "data-science",
    "mlops",
    "devops",
    "ci-cd",
    "automation",
    "scientific-computing",
    "huggingface",
    "model-analysis"
  ],
  "main": "index.js",
  "scripts": {
    "build": "napi build --platform --release",
    "build:debug": "napi build --platform",
    "test": "npm run build && node test.js",
    "examples": "npm run build && node examples.js",
    "verify": "npm run build && node -e \"console.log(require('./index.js'))\"",
    "prepublish": "npm run build",
    "artifacts": "napi artifacts"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "files": [
    "index.js",
    "*.node",
    "README.md",
    "examples.js",
    "test.js"
  ],
  "os": [
